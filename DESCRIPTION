Package: anomaly
Type: Package
Title: Detecting Anomalies in Data
Version: 4.0.2.999
Date: 2022-12-01
Authors@R: c(person("Alex","Fisch",email="a.t.fisch@lancaster.ac.uk",role=c("aut")),
             person("Daniel","Grose",email="dan.grose@lancaster.ac.uk",role=c("aut","cre")),
